{
    "version": 2,
    "builds": [
      {
        "src": "front/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "front/dist" }
      },
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/backend/index.js"
        },
        {
          "src": "/(.*)",
          "dest": "/front/dist/index.html"
        }
      ]
  }